<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><!--orange = #FFA500deeppink = #FF1493yellowgreen = #9ACD32goldenrod = #DAA520skyblue = #87CEEBchartreuse = #7FFF00magenta = #FF00FFdodgerblue = #1E90FFgold = #FFD700white = doy--><html xmlns="http://www.w3.org/1999/xhtml"><head><!-- TemplateBeginEditable name="doctitle" --><title>Tourmaline Brotherhood</title><!-- TemplateEndEditable --><link rel="Shortcut Icon" href="favicon.ico"/><script src="js/sifr.js" type="text/javascript"></script><script src="js/ext-2/adapter/ext/ext-base.js" type="text/javascript"></script><script src="js/ext-2/ext-core-debug.js" type="text/javascript"></script><script type="text/javascript">Ext.BLANK_IMAGE_URL = 'js/ext-2/resources/images/default/ext/s.gif';Ext.namespace('mannysNamespace');mannysNamespace.mannysArcanery = function() {	var	textColor = "#7FFF00";		mannysButtons = function() {		var mImage = new Image();				mImage.src = "images/tourmaline/NwNIcon.png";		mImage.src = "images/tourmaline/WoWIcon.png";	}	mannysEvents = function() {		Ext.select(".WoWLink").on("click", function() {			Ext.select("body").addClass("WoW");			Ext.select("#donimo").setDisplayed(false);			Ext.select("#aloysius").setDisplayed(true);		});		Ext.select(".NwNLink").on("click", function() {			Ext.select("body").removeClass("WoW");			Ext.select("#aloysius").setDisplayed(false);			Ext.select("#donimo").setDisplayed(true);		});	}		mannysSIFR = function() {		if (typeof sIFR == "function") {		    sIFR();			sIFR.replaceElement("#donimo .cecily h1", named({sFlashSrc: "swf/felixtitling.swf", sColor: textColor, sBgColor: "#000000", sFlashVars: "textalign=center"}));			sIFR.replaceElement("#donimo .cecily h2", named({sFlashSrc: "swf/felixtitling.swf", sColor: textColor, sBgColor: "#000000", sFlashVars: "textalign=center"}));			sIFR.replaceElement("#donimo .pandora h2", named({sFlashSrc: "swf/felixtitling.swf", sColor: textColor, sBgColor: "#000000", sFlashVars: "textalign=left"}));			sIFR.replaceElement("#donimo .pandora h3", named({sFlashSrc: "swf/felixtitling.swf", sColor: textColor, sBgColor: "#000000", sFlashVars: "textalign=left"}));			sIFR.replaceElement("#aloysius .cecily h1", named({sFlashSrc: "swf/felixtitling.swf", sColor: "#FFA500", sBgColor: "#000000", sFlashVars: "textalign=center"}));			sIFR.replaceElement("#aloysius .cecily h2", named({sFlashSrc: "swf/felixtitling.swf", sColor: "#FFA500", sBgColor: "#000000", sFlashVars: "textalign=center"}));			sIFR.replaceElement("#aloysius .pandora h2", named({sFlashSrc: "swf/felixtitling.swf", sColor: "#FFA500", sBgColor: "#000000", sFlashVars: "textalign=left"}));			sIFR.replaceElement("#aloysius .pandora h3", named({sFlashSrc: "swf/felixtitling.swf", sColor: "#FFA500", sBgColor: "#000000", sFlashVars: "textalign=left"}));		};	}		return {		conjureGallery: function() {			mannysSIFR();			mannysEvents();			mannysButtons();			Ext.select("#donimo").setDisplayed(false);		}	};}();Ext.onReady(mannysNamespace.mannysArcanery.conjureGallery, mannysNamespace.conjureGallery);</script><style type="text/css">body.WoW { border-top: 9px solid deeppink; }.WoW #ivo { border-top: 9px solid orange; }.WoW #donimo { border: 1px solid orange; }.sIFR-flash { visibility: visible !important; margin: 0; }.sIFR-replaced { visibility: visible !important; }span.sIFR-alternate { position: absolute; left: 0; top: 0; width: 0; height: 0; display: block; overflow: hidden; }.sIFR-flash + div[adblocktab=true] { display: none !important; }.sIFR-hasFlash h1 { visibility: hidden; font-size: 72px; }.sIFR-hasFlash h2 { visibility: hidden; font-size: 18px; }.sIFR-hasFlash h3 { visibility: hidden; font-size: 12px; }.sIFR-hasFlash h4 { visibility: hidden; font-size: 144px; }body { margin: 0; padding: 0; background-color: black; border-top: 9px solid magenta; }#ivo { margin: 0; padding: 0; width: 100%; height: 9px; border-top: 9px solid chartreuse; overflow: hidden; }.cecily { margin: 0 auto; width: 700px; }img { border: 0 !important; }#donimo .pandora { margin: 0 auto; width: 700px; }#aloysius .pandora { text-align: center; }#aloysius .pandora img { margin-bottom: 12px; }#aloysius .pandora h2, #aloysius .pandora h3 { margin-left: 4px; }.treasure { width: 778px; margin: 0 auto; padding: 1em; text-align: left; position: relative; }#donimo { border: 1px solid chartreuse; }#aloysius { border: 1px solid orange; }#buttons { width: 200px; position: absolute; left: -100px; }#buttons a { display: block; margin-bottom: 12px; width: 80px; height: 66px; }#buttons a:hover { cursor: pointer; }#buttons .WoWLink { margin-left: 9px; background: black url('images/tourmaline/WoWIconOff.png') no-repeat; }#buttons .WoWLink:hover { background: black url('images/tourmaline/WoWIcon.png') no-repeat; }#buttons .NwNLink { background: black url('images/tourmaline/NwNIconOff.png') no-repeat; }#buttons .NwNLink:hover { background: black url('images/tourmaline/NwNIcon.png') no-repeat; }.mannyfix:after { content: "."; display: block; height: 0; clear: both; visibility: hidden; }.mannfix { min-height: 1px; height: auto !important; height: 1px; }.yevvyLeft { margin-right: 12px; }.mannyLeft { text-align: right; }.xeCenter { vertical-align: top; }.xeLeft, .xeRight { width: 100%; }.xeLeft { text-align: left; }.xeRight { text-align: right; }.WoWTable td { width: 50%; }.goodbye { margin-top: 198px; }</style><!-- TemplateBeginEditable name="head" --><!-- TemplateEndEditable --></head><body><div id="ivo"></div><div id="aloysius" class="treasure" ></div><br/></body></html>