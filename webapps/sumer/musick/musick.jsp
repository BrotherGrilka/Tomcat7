<script type="text/javascript">
musickPlayer = function() {
	var yH, mArtist, mTitle, mFile, mWidth = "300", mHeight = "20", mColor, mPlayer = '', yPlayer = '', pPlayer = '';
	var mIsIe = navigator.userAgent.indexOf("MSIE") != -1 ? true : false;
	
	getPlayer = function(movieName) {
		if (window.document[movieName]) 
			return window.document[movieName];
	
		if (navigator.appName.indexOf("Microsoft Internet") == -1) {
			if (document.embeds && document.embeds[movieName])
				return document.embeds[movieName]; 
		}
		else
			return document.getElementById(movieName);
	}
	
	exvanirePlayer = function(holder) {
		$('gHolder').update('<div id="plInfo">' + mArtist + '<br><span id="plTitle">' + mTitle + '</span></div><div id="tHolder"></div>');
		$('tHolder').observe('click', function() {
			new Effect.Move($(holder), {x: -1200, y: 0, mode: 'relative', duration: 0.3, afterFinish: function() {
				if (yPlayer != '')
					yPlayer.disConnect();
				yPlayer = '';
				yH.innerHTML = '';
				yH.remove();
				mPlayer = '';
				pPlayer = '';
			}});
		});	
	}
	
	wmaPlaya = function(params) {
		pPlayer = 'wma';
		if (mPlayer == '') {
			yH = new Element('div', {id: 'yHolder'});

			if (mIsIe) {
				var vHeight = document.viewport.getHeight() + document.viewport.getScrollOffsets().top - 56;
				yH.setStyle({top: vHeight});
			}
		
			new Insertion.Top(document.body, yH);
			var mH = new Element('div', {id: 'mHolder'});
			var gH = new Element('div', {id: 'gHolder'});
			new Insertion.Top($('yHolder'), mH);
			new Insertion.Top($('yHolder'), gH);

			mColor = 'FC1900';

			switch (params.skin) {
				case 'yellow':
					mColor = '333333';
					$('gHolder').addClassName('mYellow');
					break;
				case 'green':
					mColor = '9BB400';
					$('gHolder').addClassName('mGreen');
					break;
				case 'red':
					mColor = 'FC1900';
					$('gHolder').addClassName('mRed');
					$('mHolder').addClassName('sRed');
					break;
				case 'blue':
					mColor = '02B5D6';
					$('gHolder').addClassName('mBlue');
					break;
				default:
					$('gHolder').addClassName('mRed');
			}
				
			new Draggable('yHolder', {endeffect: ''});
			new Effect.Move($('yHolder'), {x: 400, y: 0, mode: 'relative', duration: 0.3});
		}

		mArtist = (params.artist == '') ? 'Atomic Rooster' : params.artist;
		mTitle = (params.title == '') ? 'Start To Live' : params.title;

		mFile = 'http://sg1.allmusic.com/cg/smp.dll?link=' + params.file + '&r=20.asx';

		var mConfig = {
			file: mFile,
			autostart: 'true',
			usefullscreen: 'false',
			frontcolor: mColor,
			height: mHeight,
			width: mWidth
		};

		mPlayer = new jeroenwijering.Player($("mHolder"), 'wmvplayer.xaml', mConfig);

		if (!Silverlight.available) {
			if (mIsIe)
				yH.setStyle({top: vHeight - 120});
			else
				yH.setStyle({bottom: "20%"});
			$("mHolder").setStyle({backgroundColor: mColor}).addClassName("noSilverlight").update(
				"<a href='http://silverlight.net/' target='_blank'><img src='http://www.billboard.com/bbcom/samples/images/getsilverlight.png'></a><br><br>Or listen to <a href='" + mFile + "' target='_blank'>" + params.title + "</a> now"
			);
			mPlayer = true;
		}

		exvanirePlayer('yHolder');
	}

	mp3Playa = function(params) {
		pPlayer = 'mp3';
		if (mPlayer == '') {
			yH = new Element('div', {id: 'pHolder'});

			if (mIsIe) {
				var vHeight = document.viewport.getHeight() + document.viewport.getScrollOffsets().top - 176;
				yH.setStyle({top: vHeight});
			}
		
			new Insertion.Top(document.body, yH);
			var mH = new Element('div', {id: 'mHolder'});
			var gH = new Element('div', {id: 'gHolder'});
			new Insertion.Top($('pHolder'), mH);
			new Insertion.Top($('pHolder'), gH);

			mColor = 'FC1900';

			switch (params.skin) {
				case 'green':
					mColor = 'green';
					$('pHolder').addClassName('pGreen');
					break;
				case 'red':
					mColor = 'red';
					$('pHolder').addClassName('pRed');
					$('mHolder').addClassName('sRed');
					break;
				default:
					$('pHolder').addClassName('pRed');
			}
				
			new Draggable('pHolder');
			new Effect.Move($('pHolder'), {x: 410, y: 0, mode: 'relative', duration: 0.3});
		}

		mArtist = params.artist;
		mTitle = params.title;

		mPlayer = new SWFObject("sonicGuide.swf", "sonicGuide", mWidth, mHeight, "7");
		mPlayer.addVariable("width", mWidth);
		mPlayer.addVariable("height", mHeight);
		mPlayer.addVariable("autostart", true);
		mPlayer.addVariable("trackId", params.file);
		mPlayer.addVariable("skinColor", mColor);
		mPlayer.addParam("wmode", "transparent");
		mPlayer.addParam("name", "sonicGuide");
		mPlayer.write("mHolder");

		exvanirePlayer('pHolder');
		
		yPlayer = getPlayer("sonicGuide");
	}

	return {
		conjurePlayer : function(params) {
			if (params.format == 'wma' && pPlayer != 'mp3')
				wmaPlaya(params);
			else if (params.format == 'mp3' && pPlayer != 'wma')
				mp3Playa(params);
		}
	};
}();
</script>
